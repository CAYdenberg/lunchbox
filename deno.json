{
  "name": "@lunchbox/ui",
  "version": "1.0.35",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts",
    "./utils": "./src/utils.ts",
    "./tailwind": "./src/tailwind.ts",
    "./particles": "./src/particles.ts",
    "./atoms": "./src/atoms.tsx",
    "./molecules/Accordion": "./src/molecules/Accordion.tsx",
    "./molecules/FieldsetCheck": "./src/molecules/FieldsetCheck.tsx",
    "./molecules/FieldsetRadio": "./src/molecules/FieldsetRadio.tsx",
    "./molecules/InputCheckCombo": "./src/molecules/InputCheckCombo.tsx",
    "./molecules/InputFieldCombo": "./src/molecules/InputFieldCombo.tsx",
    "./molecules/InputRadioCombo": "./src/molecules/InputRadioCombo.tsx",
    "./molecules/InputSelectCombo": "./src/molecules/InputSelectCombo.tsx",
    "./molecules/InputTextareaCombo": "./src/molecules/InputTextareaCombo.tsx",
    "./molecules/Markdown": "./src/molecules/Markdown.tsx"
  },
  "tasks": {
    "base:start": "deno run -A --watch=static/,routes/ examples/base/dev.ts",
    "init:generate": "deno run -A src/tasks.ts init-generate",
    "init:clean": "deno run -A src/tasks.ts init-clean",
    "init:start": "deno run -A --watch=static/,routes/ examples/init/dev.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "fmt": { "exclude": [".github/dep", "*.md"], "singleQuote": true },
  "imports": {
    "@std/cli": "jsr:@std/cli@^1.0.14",
    "@std/fmt": "jsr:@std/fmt@^1.0.6",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@deno/gfm": "jsr:@deno/gfm@^0.10.0",
    "@vyn/cn": "jsr:@vyn/cn@^0.1.2",
    "preact": "npm:preact@^10.24.1",
    "tailwindcss/plugin": "npm:/tailwindcss@3.4.1/plugin.js"
  }
}
